<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_search">

    <fragment
        android:id="@+id/navigation_search"
        android:name="de.produktsuche.ui.search.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/content_list" />

    <fragment
        android:id="@+id/navigation_watchlist"
        android:name="de.produktsuche.ui.watchlist.WatchlistFragment"
        android:label="@string/title_watchlist"
        tools:layout="@layout/content_list" />

    <fragment
        android:id="@+id/navigation_reserved_list"
        android:name="de.produktsuche.ui.reserved_list.Reserved_ListFragment"
        android:label="@string/title_reserved_list"
        tools:layout="@layout/content_list" />

    <fragment
        android:id="@+id/navigation_login"
        android:name="de.produktsuche.ui.login.LoginFragment"
        android:label="@string/title_login"
        tools:layout="@layout/fragment_login"
        />
</navigation>